{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "lib": [
      "es2016"
    ],
    "outDir": "./dist",
    "strictNullChecks": true,
    "noImplicitUseStrict": true,
    "esModuleInterop": true,
    "noEmitOnError": true,
    "strictPropertyInitialization": true
  },
  "include": [
    "./src"
  ]
}
